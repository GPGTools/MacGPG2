#define EXEEXT "$EXEEXT"
_ACEOF


if test x"$try_hkp" = xyes ; then
  GPGKEYS_HKP="gpg2keys_hkp$EXEEXT"

fi

if test x"$try_finger" = xyes ; then
  GPGKEYS_FINGER="gpg2keys_finger$EXEEXT"

fi



