<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="1">
    <pkg-ref id="net.sourceforge.gpgosx"/>
    <title>GnuPG for OS X</title>
    <options customize="never"
      require-scripts="false"
      hostArchitectures="x86_64"
      rootVolumeOnly="true"/>
    <choices-outline>
        <line choice="default">
            <line choice="net.sourceforge.gpgosx"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="net.sourceforge.gpgosx" visible="false">
        <pkg-ref id="net.sourceforge.gpgosx"/>
    </choice>
    <pkg-ref id="net.sourceforge.gpgosx" version="{PKGVERSION}" onConclusion="none">GnuPG.pkg</pkg-ref>
</installer-gui-script>
