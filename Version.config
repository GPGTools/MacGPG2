
temp=$(sed -nE '/.*gnupg-2\.0\.([0-9]+)\.tar.bz2.*/{s//\1/;p;q;}' < "$(dirname "${BASH_SOURCE[0]}")/Formula/MacGPG2.rb")
if [[ -z "$temp" ]] ;then
	echo "Unable to find MacGPG2 revision! Possible we are on 2.1?" >&2
	exit 1
fi

MAJOR=2
MINOR=0
REVISION=$temp
PRERELEASE=b5

VERSION="${MAJOR}.${MINOR}${REVISION:+.$REVISION}${PRERELEASE}"
