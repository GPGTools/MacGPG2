rm -f $OldMacGPG2/share/gnupg/com-certs.pem &> /dev/null
rm -f $OldMacGPG2/share/gnupg/gpg-conf.skel &> /dev/null
rm -f $OldMacGPG2/share/gnupg/help.be.txt &> /dev/null
rm -f $OldMacGPG2/share/gnupg/help.ca.txt &> /dev/null
