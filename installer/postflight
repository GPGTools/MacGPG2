rm -f $OldMacGPG2/share/locale/de &> /dev/null
rm -f $OldMacGPG2/share/locale/de/LC_MESSAGES &> /dev/null
rm -f $OldMacGPG2/share/locale/de/LC_MESSAGES/gnupg2.mo &> /dev/null
