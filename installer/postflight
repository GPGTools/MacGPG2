rm -f $OldMacGPG2/share/doc/gnupg/examples/gpgconf.conf &> /dev/null
rm -f $OldMacGPG2/share/doc/gnupg/examples/pwpattern.list &> /dev/null
rm -f $OldMacGPG2/share/doc/gnupg/examples/README &> /dev/null
